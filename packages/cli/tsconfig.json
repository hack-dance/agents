{
  "extends": "../tsconfig/base.json",
  "compilerOptions": {
    "incremental": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts",
    "src/*.ts",
    "src/*.tsx"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ]
}
